{
  "name": "sidetrek",
  "version": "1.0.0",
  "description": "Sidetrek App",
  "main": "index.js",
  "engines": {
    "node": "11.9.0"
  },
  "scripts": {
    "start": "node dist/index.js",
    "server": "env-cmd config/dev.env nodemon src/index.js --ext js,graphql --exec babel-node",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "get-schema": "graphql get-schema -p prisma --dotenv config/dev.env",
    "deploy:prisma:dev": "cd prisma && prisma deploy -e ../config/dev.env && cd .. && npm run get-schema",
    "deploy:prisma:test": "cd prisma && prisma deploy -e ../config/test.env",
    "deploy:prisma:prod": "cd prisma && prisma deploy -e ../config/prod.env",
    "test": "env-cmd config/test.env jest --watch --runInBand",
    "heroku-postbuild": "babel src --out-dir dist --copy-files && npm install --prefix client && npm run build --prefix client"
  },
  "jest": {
    "globalSetup": "./tests/jest/globalSetup.js",
    "globalTeardown": "./tests/jest/globalTeardown.js"
  },
  "author": "Seungchan Lee",
  "license": "ISC",
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "apollo-boost": "^0.1.27",
    "auth0": "^2.14.0",
    "auth0-js": "^9.9.1",
    "aws-sdk": "^2.399.0",
    "babel-register": "^6.26.0",
    "compression": "^1.7.3",
    "concurrently": "^4.1.0",
    "cors": "^2.8.5",
    "cross-fetch": "^2.2.2",
    "env-cmd": "^8.0.2",
    "express": "^4.16.4",
    "graphql": "^0.13.2",
    "graphql-cli": "^3.0.5",
    "graphql-yoga": "^1.17.0",
    "jsonwebtoken": "^8.4.0",
    "jwks-rsa": "^1.3.0",
    "node-fetch": "^2.3.0",
    "passport": "^0.4.0",
    "passport-google-oauth": "^1.0.0",
    "prisma-binding": "^2.2.14",
    "ramda": "^0.26.1",
    "subscriptions-transport-ws": "^0.9.15",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "jest": "^23.6.0",
    "nodemon": "^1.18.9"
  },
  "resolutions": {
    "graphql": "^0.13.2"
  }
}
